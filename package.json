{
  "name": "reactapp",
  "version": "0.0.1",
  "description": "React App Boilerplate",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server",
    "build": "webpack",
    "flow": "flow",
    "std": "standard --verbose | snazzy",
    "stdf": "standard --fix",
    "format": "prettier-standard 'src/**/*.js'",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skeep/reactapp.git"
  },
  "author": "Suman Paul",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skeep/reactapp/issues"
  },
  "homepage": "https://github.com/skeep/reactapp#readme",
  "devDependencies": {
    "@storybook/react": "^3.3.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-flow-props-to-prop-types": "^0.15.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.11.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.59.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "prettier-standard": "^8.0.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.1.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.0",
    "prop-types-extra": "^1.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "it",
      "expect"
    ]
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  }
}
